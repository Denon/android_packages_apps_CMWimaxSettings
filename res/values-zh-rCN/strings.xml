    <string name="wimax_status_disconnecting">正在中断连接…</string>
    <!-- Status message of wimax when it is disconnected -->
    <string name="wimax_status_disconnected">已中断连接</string>

    <!-- About phone, status item value if the actual value is not available. -->
    <string name="status_unavailable">无法使用</string>

